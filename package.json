{
  "name": "jekyll-rollup-js",
  "version": "1.0.0",
  "description": "Rollup js with Jekyll",
  "main": "/dist/index.js",
  "author": "Fouad",
  "license": "UNLICENSED",
  "scripts": {
    "watch": "bundle exec jekyll serve --livereload",
    "build": "yarn run build:scripts && bundle exec jekyll build",
    "build:scripts": "rollup -c",
    "watch:scripts": "yarn run build:scripts -w"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "glob": "^7.1.6",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
